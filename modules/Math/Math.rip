$ #include <math.h> $

module Math
  bit_or(a, b)
    return $ int64_to_val(val_to_int64(__a) | val_to_int64(__b)) $

  bit_and(a, b)
    return $ int64_to_val(val_to_int64(__a) & val_to_int64(__b)) $


  sqrt(x)
    return $ double_to_val(sqrt(val_to_double(__x))) $

  ceil(x)
    return $ int64_to_val((int64) ceil(val_to_double(__x))) $
